{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/components/*": ["./app/components/*"],
      "@/public/*": ["./public/*"],
      "@/scripts/*": ["./public/scripts/*"],
      "@/styles/*": ["./public/css/*"],
      "@/api/*": ["./app/api/*"],
      "@/global": ["./app/global.css"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/components/server.tsx"
, "app/api/database/dbHandler.tsx" ],
  "exclude": [
    "node_modules"
  ]
}
